<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:background="#000000"
		android:gravity="center_horizontal|center_vertical"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear18"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="4dp"
			android:paddingLeft="40dp"
			android:paddingTop="80dp"
			android:gravity="center_vertical"
			android:orientation="horizontal">
			<ImageView
				android:id="@+id/imageview11"
				android:layout_width="25dp"
				android:layout_height="25dp"
				android:src="@drawable/back"
				android:scaleType="centerInside" />
			<TextView
				android:id="@+id/textview11"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="back"
				android:textSize="16sp"
				android:textStyle="bold"
				android:textColor="#E7E1E5" />
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear19"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="vertical">
			<TextView
				android:id="@+id/textview1"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:text="No account"
				android:textSize="30sp"
				android:textStyle="bold"
				android:textColor="#E7E1E5" />
			<TextView
				android:id="@+id/textview8"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginLeft="40dp"
				android:layout_marginTop="8dp"
				android:layout_marginRight="40dp"
				android:layout_marginBottom="8dp"
				android:paddingLeft="8dp"
				android:gravity="center_horizontal|center_vertical"
				android:text="Sign up to create circles with friends."
				android:textSize="14sp"
				android:textColor="#E7E1E5"
				android:alpha="0.5" />
			<LinearLayout
				android:id="@+id/linear16"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="8dp"
				android:paddingLeft="64dp"
				android:paddingTop="12dp"
				android:paddingRight="64dp"
				android:paddingBottom="12dp"
				android:background="#E7B4F9"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview10"
					android:layout_width="20dp"
					android:layout_height="20dp"
					android:layout_marginRight="4dp"
					android:src="@drawable/user"
					android:scaleType="centerInside" />
				<TextView
					android:id="@+id/textview9"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Create account"
					android:textSize="12sp"
					android:textStyle="bold"
					android:textColor="#471E58" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear20"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginTop="8dp"
				android:paddingLeft="64dp"
				android:paddingTop="12dp"
				android:paddingRight="64dp"
				android:paddingBottom="12dp"
				android:background="#471E58"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<ImageView
					android:id="@+id/imageview12"
					android:layout_width="20dp"
					android:layout_height="20dp"
					android:layout_marginRight="4dp"
					android:src="@drawable/log_in"
					android:scaleType="centerInside" />
				<TextView
					android:id="@+id/textview12"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:text="Sign in"
					android:textSize="12sp"
					android:textStyle="bold"
					android:textColor="#E7E1E5" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
